{
  "name": "shorthand-tutor-app",
  "version": "1.0.0",
  "type": "module",
  "description": "Interactive Pitman Shorthand learning platform with comprehensive NCS curriculum, speed development, AI-powered feedback, and professional certification for stenography students and court reporters",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "test": "vitest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "share": "npm run build && npm run preview",
    "deploy": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build",
    "seo-test": "npm run build && npx serve dist -p 3000"
  },
  "homepage": "https://tmaenge-dot.github.io/shorthand-tutor-app",
